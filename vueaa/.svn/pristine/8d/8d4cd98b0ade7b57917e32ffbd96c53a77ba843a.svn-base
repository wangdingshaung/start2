import Vue from 'vue'
import Router from 'vue-router'
import goods from '@/components/goods'
import bokeliebiao from '@/components/bokeliebiao'
import laowang from '@/components/laowang'


Vue.use(Router);

export default new Router({
  routes: [
    {
      path: '/goods',
      name: 'goods',
      component: goods
    },{
      path: '/',
      name: 'bokeliebiao',
      component: bokeliebiao
    },
    {
      path: '/laowang',
      name: 'laowang',
      component: laowang
    }
  ]
})
